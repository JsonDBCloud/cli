{
  "name": "@jsondb-cloud/cli",
  "version": "1.0.0",
  "description": "CLI tool for jsondb.cloud â€” push, pull, manage your JSON database from the terminal",
  "license": "MIT",
  "main": "dist/index.js",
  "bin": {
    "jsondbcloud": "bin/jsondb.js",
    "jsondb": "bin/jsondb.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "test": "vitest run"
  },
  "dependencies": {
    "commander": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^20.0.0",
    "eslint": "^10.0.2",
    "prettier": "^3.8.1",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.56.1",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "jsondb",
    "cli",
    "json",
    "database",
    "api",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsonDBCloud/cli.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
